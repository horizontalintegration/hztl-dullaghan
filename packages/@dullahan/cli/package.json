{
  "name": "@dullahan/cli",
  "version": "0.1.4",
  "description": "Command line tools for headless development",
  "type": "module",
  "keywords": [
    "CLI",
    "Sitecore JSS"
  ],
  "contributors": [
    "Brian George <bgeorge@horizontal.com>"
  ],
  "homepage": "https://github.com/horizontalintegration/hztl-dullahan/tree/main/packages/@dullahan/cli#readme",
  "license": "ISC",
  "main": "lib/cli.js",
  "bin": {
    "dullahan": "lib/cli.js"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/horizontalintegration/hztl-dullahan.git"
  },
  "scripts": {
    "test": "npm run build && npm run local && cd ../../../ && dullahan create test",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' -e ts",
    "build": "rm -rf ./lib && tsc --project tsconfig.json",
    "local": "sudo npm i -g",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "bugs": {
    "url": "https://github.com/horizontalintegration/hztl-dullahan/issues"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "commander": "^9.0.0",
    "execa": "^6.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.21",
    "fs-extra": "^10.0.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "gitHead": "509e358b3c37b09672f495d300dc2077e5bf0b6f"
}
